    <script>
        // Fetch progress data from the server
        fetch('/user-progress-data')
            .then(response => response.json())
            .then(userProgress => {
                const dates = userProgress.map(entry => new Date(entry.date).toDateString());
                const caloriesBurnedData = userProgress.map(entry => entry.caloriesBurned);
                const fatLossData = userProgress.map(entry => entry.fatLoss);
                const weightChangeData = userProgress.map(entry => entry.weightChange);
                const dailyActivityData = userProgress.map(entry => entry.dailyActivity);

                // Create a bar chart
                const ctx = document.getElementById('progressChart').getContext('2d');
                new Chart(ctx, {
                    type: 'bar',
                    data: {
                        labels: dates,
                        datasets: [
                            {
                                label: 'Calories Burned',
                                data: caloriesBurnedData,
                                backgroundColor: 'rgba(139, 0, 0, 0.8)', // Dark Red
                            },
                            {
                                label: 'Fat Loss',
                                data: fatLossData,
                                backgroundColor: 'rgba(0, 0, 139, 0.8)', // Dark Blue
                            },
                            {
                                label: 'Weight Change',
                                data: weightChangeData,
                                backgroundColor: 'rgba(128, 128, 0, 0.8)', // Dark Olive
                            },
                            {
                                label: 'Daily Activity Level',
                                data: dailyActivityData,
                                backgroundColor: 'rgba(46, 139, 87, 0.8)', // Dark Green
                            },
                        ],
                    },
                    options: {
                        scales: {
                            x: {
                                stacked: true,
                            },
                            y: {
                                stacked: true,
                            },
                        },
                    },
                });
            })
            .catch(error => console.error('Error fetching user progress:', error));
    </script>
