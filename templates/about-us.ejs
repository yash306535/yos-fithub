                    <span>Testimonial</span>
                    <h2>Our clients say</h2>
                </div>
            </div>
        </div>
        <div class="ts_slider owl-carousel">
            <% feedbacks.forEach((feedback) => { %>
                <div class="ts_item">
                    <div class="row">
                        <div class="col-lg-12 text-center">
                            <!-- Display feedback details -->
                            <div class="ti_pic">
                                <!-- Adjust the image source based on your feedback schema -->
                                <img src="<%= feedback.imageSrc %>" alt="<%= feedback.name %>">
                            </div>
                            <div class="ti_text">
                                <p><%= feedback.feedback %></p>
                                <h5><%= feedback.name %></h5>
                                <div class="tt-rating">
                                    <% for (let i = 0; i < feedback.rating; i++) { %>
                                        <i class="fa fa-star"></i>
                                    <% } %>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            <% }) %>
        </div>
    </div>
</section>

