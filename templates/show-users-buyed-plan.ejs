    <div class="container1">
        <h1 class="heading">Users who Bought the Plan</h1>
        
        <% if (users.length > 0) { %>
            <% users.forEach(user => { %>
                <div class="user-card">
                    <h3>Name: <%= user.name %></h3>
                    <p>Email: <%= user.email %></p>
                    
                    <!-- Display all purchased plans for this user -->
                    <h3>Purchased Plans:</h3>
                    <ul class="purchased-plans">
                        <% user.purchasedPlans.forEach(plan => { %>
                            <li class="plan-card">
                                <p>Plan Name: <%= plan.name %></p>
                                <p>Price: $<%= plan.price %></p>
                                <!-- Add other plan details you want to display -->
                            </li>
                        <% }) %>
                    </ul>

                    <!-- Contact User Button -->
                    <button class="contact-user-btn" onclick="contactUser('<%= user.email %>')">Contact User</button>
                    <button class="contact-user-btn" onclick="addProgress1()">Show Progress</button>

                </div>
            <% }) %>
        <% } else { %>
            <p>No users found who bought the plan.</p>
