    <!-- Offcanvas Menu Section Begin -->
    <%- include ('admin_header') %>


<section class="hero-section">
    <div class="hs-slider owl-carousel">
        <div class="hs-item set-bg" data-setbg="img/hero/hero-1.jpg" style="height: fit-content;">
            
        </div>
    </div>
</section>
<!-- Pricing Section End -->


<div class="container1" >
    <h1>All Trainers</h1>
    <div class="trainer-container">
        <% trainers.forEach(trainer => { %>
            <div class="trainer-card">
                <% if (trainer.profileImage1) { %>
                    <img src="/trainerimg/<%= trainer.profileImage1 %>" alt="Trainer Photo">
                <% } else { %>
                    <!-- You can provide a default image if profileImage1 is not available -->
                    <img src="default-profile-image.jpg" alt="Default Photo">
                <% } %>
                <h2><%= trainer.trainerName %></h2>
                <p>Email: <%= trainer.trainerEmail %></p>
                <p>Specialization: <%= trainer.trainerSpecialization %></p>
                <!-- Add more details as needed -->
                <a href="/show-trainer-to-user/<%= trainer._id %>">View Profile</a>
            </div>
        <% }) %>
    </div>
</div>
