    <div class="container1">
        <h1 id="myPlansHeading">My Plans</h1>
    
        <% if (plans.length === 0) { %>
            <p class="no-plans-message">No plans added yet.</p>
        <% } else { %>
            <div class="plans-container">
                <% plans.forEach((plan, index) => { %>
                    <div class="plan-card" id=<%= "planCard" + index %> >
                        <ul class="plan-details">
                            <img src="<%= plan.trainerProfileImage %>" alt="Trainer Profile Image" class="trainer-profile-image">
                            <li>
                                <strong>Name:</strong> <span class="plan-name"><%= plan.name %></span>
                            </li>
                            <li>
                                <strong>Price:</strong> <span class="plan-price"><%= plan.price %></span>
                            </li>
                            <li>
                                <strong>Diet:</strong> <span class="plan-diet"><%= plan.diet %></span>
                            </li>
                            <li>
                                <strong>Exercise:</strong> <span class="plan-exercise"><%= plan.exercise %></span>
                            </li>
                            <li>
                                <strong>Duration:</strong> <span class="plan-duration"><%= plan.duration %></span>
                            </li>
                            <li>
                                <strong>Trainer:</strong> <span class="plan-trainer"><%= plan.trainer %></span>
                            </li>
                            <!-- Additional information -->
                            <li>
                                <strong>Special Designed For:</strong> <span class="plan-special-designed-for"><%= plan.specialDesignedFor %></span>
                            </li>
                            <li>
                                <strong>Age Group:</strong> <span class="plan-age-group"><%= plan.ageGroup %></span>
                            </li>
                            <li>
                                <strong>Category:</strong> <span class="plan-category"><%= plan.category %></span>
                            </li>
                            <li>
                                <strong>Intensity:</strong> <span class="plan-intensity"><%= plan.intensity %></span>
                            </li>
                            <li>
                                <strong>Required Equipment:</strong> <span class="plan-required-equipment"><%= plan.requiredEquipment %></span>
                            </li>
                            <li>
                                <strong>Target:</strong> <span class="plan-target"><%= plan.target %></span>
                            </li>
                            <li>
                                <strong>Difficulty:</strong> <span class="plan-difficulty"><%= plan.difficulty %></span>
                            </li>
                            <li>
                                <strong>Frequency:</strong> <span class="plan-frequency"><%= plan.frequency %></span>
                            </li>
                            <li>
                                <strong>Benefits:</strong> <span class="plan-benefits"><%= plan.benefits %></span>
                            </li>
                            <li>
                                <button class="contact-user-btn" data-user-email="<%= plan.userEmail %>">Contact User</button>
                            </li>
                        </ul>
                    </div>
                <% }); %>
            </div>
        <% } %>
